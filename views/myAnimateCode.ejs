function myItem1Animate(myElement) {
    myDelay = myDelay + myDelayIncrement;
    myElement.animate( {top: "+=100px"} ).delay(myDelay).show(1000)
	.animate({left: '40%'}, 1000)
	.animate({fontSize: '2em', color: 'red'}, 500)
	.animate( {top: "+=50px"}, 1100 )
	.delay(500)
	.animate( {top: "-=25px"}, 1100 )
	.animate({fontSize: '1em', color: 'black'}, 500)
	.animate({fontSize: '3em', color: 'blue'}, 500, item1looping());
}

function myItem10Animate(myElement) {
    myDelay = myDelay + myDelayIncrement;
    myElement.animate( {top: "+=100px"} ).delay(myDelay).show(1000)
	.animate({left: '40%'}, 1000)
	.animate({fontSize: '2em', color: 'red'}, 500)
	.animate( {top: "+=500px"}, 1100, item10rotate() );
